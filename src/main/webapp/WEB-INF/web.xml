<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">
    <servlet>
        <servlet-name>VerificationServlet</servlet-name>
        <servlet-class>controller.VerificationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VerificationServlet</servlet-name>
        <url-pattern>/VerificationServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ClientListServlet</servlet-name>
        <servlet-class>controller.ClientListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClientListServlet</servlet-name>
        <url-pattern>/ClientListServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AccountTransactionServlet</servlet-name>
        <servlet-class>controller.AccountTransactionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccountTransactionServlet</servlet-name>
        <url-pattern>/AccountTransactionServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NewFormServlet</servlet-name>
        <servlet-class>controller.NewFormServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewFormServlet</servlet-name>
        <url-pattern>/NewFormServlet</url-pattern>
    </servlet-mapping>
</web-app>
